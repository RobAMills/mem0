# API Keys
# Z.AI API key (default LLM provider)
ZAI_API_KEY=your-zai-api-key

# OpenAI API key (optional, if using OpenAI instead of Z.AI)
OPENAI_API_KEY=

USER=user

# Categorization settings
ENABLE_CATEGORIZATION=false
# Options: zai, openai, ollama
CATEGORIZATION_PROVIDER=zai
# Model to use for categorization (provider-specific)
# Z.AI: glm-4.7, glm-4.6, glm-4.5
# OpenAI: gpt-4o-mini, gpt-4o
# Ollama: phi3:mini, llama3, mistral
CATEGORIZATION_MODEL=glm-4.7